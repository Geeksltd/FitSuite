{
  "App.Resource.Version": "1.0.0",
  "PermittedDomains": "http://visualspec.local.com",
  "Microservice": {
    "Me": {
      "Name": "Hub"
    },
    "Hub": {
      "Url": "http://hub.visualspec.local.com:8685/",
      "AccessKey": "local-hub"
    },
    "Scope": {
      "Url": "http://scope.visualspec.local.com:8659/",
      "AccessKey": "local-hub"
    },
    "Stories": {
      "Url": "http://stories.visualspec.local.com:8658/",
      "AccessKey": "local-hub"
    },
    "Estimator": {
      "Url": "http://estimator.visualspec.local.com:8625/",
      "AccessKey": "local-hub"
    }

  },
  "SecuritySettings": {
    "DevelopmentMode": "true",
    "Google": "true"
  },
  "Authentication": {
    "Cookie": {
      "Timeout": 540,
      "TimeoutMobile": 525600
    },
    "Timeout": 1430,
    "Google": {
      "ClientId": "[#SET#]",
      "ClientSecret": "[#SET#]"
    }
  },
  "DataReplication": {
    "PeopleService.HubEndPoint": {
      "Url": "https://sqs.eu-west-1.amazonaws.com/669486994886/PeopleService-HubEndPoint.fifo"
    }
  },
  "Database": {
    "StoragePath": "C:\\@Database.Files",
    "Profile": false,
    "Cache": {
      "Mode": "single-server",
      "ConcurrencyAware": true
    },
    "Transaction": {
      "Type": "Olive.Entities.Data.DbTransactionScope, Olive.Entities.Data",
      "DefaultIsolationLevel": "ReadCommitted",
      "EnforceForSave": true
    },
    "Audit": {
      "Insert": {
        "Action": false,
        "Data": false
      },
      "Update": {
        "Action": false,
        "Data": false
      },
      "Delete": {
        "Action": false,
        "Data": false
      }
    }
  },

  "Default.TransactionScope.Type": "Olive.Entities.Data.DbTransactionScope, Olive.Entities.Data,",
  "Default.Transaction.IsolationLevel": "ReadCommitted",
  "Blob": {
    "RootPath": "Blob",
    "BaseUrl": "/file?",

    "WebTest": {
      "SuppressPersistence": false,
      "Origin": "..\\Test\\ReferenceFiles"
    }
  },
  "GlobalSearch": {
    "Sources": [
      "Hub/api/global-search",
      "Scope/api/global-search",
      "Stories/api/global-search",
      "Estimator/api/global-search"
    ]
  },

  "Translate.Validators": false,

  "Automated.Tasks": {
    "Enabled": true
  }


}


